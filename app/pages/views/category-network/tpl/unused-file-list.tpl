<div class="row">
    <div class="col-12 mt-2 wiki_column">
        <table>
            <tbody>
            {{#each files}}
            <tr>
                <td style="padding-left:2em">
                    <a href="../file/{{this}}" style="font-size:0.8em;text-overflow: ellipsis">{{this}}</a>
                </td>
            </tr>
            {{/each}}
            </tbody>
        </table>
    </div>
</div>